{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "threads": {
      "type": "integer"
    },
    "anon_metrics_enable": {
      "type": "boolean"
    },
    "deemix": {
      "type": "object",
      "properties": {
        "arl": {
          "type": "string"
        },
        "download_path": {
          "type": "string"
        },
        "max_bitrate": {
          "type": "string"
        },
        "skip_low_quality": {
          "type": "boolean"
        }
      },
      "required": [
        "arl",
        "download_path",
        "max_bitrate",
        "skip_low_quality"
      ]
    },
    "spotify": {
      "type": "object",
      "properties": {
        "custom_application": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "client_id": {
              "type": "string"
            },
            "client_secret": {
              "type": "string"
            },
            "scope": {
              "type": "string"
            },
            "redirect_uri_port": {
              "type": "string"
            }
          },
          "required": [
            "enabled",
            "client_id",
            "client_secret",
            "scope",
            "redirect_uri_port"
          ]
        },
        "username": {
          "type": "string"
        },
        "playlists": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "owner_only": {
              "type": "boolean"
            },
            "excluded": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "enabled",
            "owner_only",
            "excluded"
          ]
        }
      },
      "required": [
        "custom_application",
        "username",
        "playlists"
      ]
    },
    "notification": {
      "type": "object",
      "properties": {
        "strategy": {
          "type": "integer"
        },
        "provider": {
          "type": "object",
          "properties": {
            "pushover": {
              "type": "object",
              "properties": {
                "enabled": {
                  "type": "boolean"
                },
                "user_key": {
                  "type": "string"
                },
                "api_token": {
                  "type": "string"
                }
              },
              "required": [
                "enabled",
                "user_key",
                "api_token"
              ]
            }
          },
          "required": [
            "pushover"
          ]
        }
      },
      "required": [
        "strategy",
        "provider"
      ]
    },
    "autoscan": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "endpoint": {
          "type": "string"
        },
        "auth_enabled": {
          "type": "boolean"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": [
        "enabled",
        "endpoint",
        "auth_enabled",
        "username",
        "password"
      ]
    }
  },
  "required": [
    "threads",
    "anon_metrics_enable",
    "deemix",
    "spotify",
    "notification",
    "autoscan"
  ]
}